{
  "name": "OldStyle Copy&Paste",
  "description": "OldStyle Copy&Paste",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "run_at": "document_idle",
      "js": ["old-style.js"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
      }
  },
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  }
}
